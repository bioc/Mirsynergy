	Eprint = {http://cancerres.aacrjournals.org/content/70/23/9570.full.pdf+html},
	Journal = {Cancer Research},
	Number = {23},
	Pages = {9570-9580},
	Title = {MicroRNA miR-183 Functions as an Oncogene by Targeting the Transcription Factor EGR1 and Promoting Tumor Cell Migration},
	Url = {http://cancerres.aacrjournals.org/content/70/23/9570.abstract},
	Volume = {70},
